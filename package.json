    "@polkadot/ui-settings": "^0.86.5",
    "@polkadot/util": "^7.8.2",
    "@polkadot/util-crypto": "^7.8.2",
    "@reduxjs/toolkit": "^1.7.1",
    "classnames": "^2.3.1",
    "moment": "^2.29.1",
    "node-sass": "^7.0.1",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-big-calendar": "^0.38.4",
